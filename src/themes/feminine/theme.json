{
  "id": "feminine",
  "name": "Feminine Blush",
  "colors": {
    "bg": "#fff8f9",
    "fg": "#4a2c3a",
    "primary": "#e91e63",
    "primaryFg": "#ffffff",
    "secondary": "#f48fb1",
    "secondaryFg": "#ffffff",
    "muted": "#fce4ec",
    "mutedFg": "#ad7a8a",
    "success": "#4caf50",
    "warning": "#ff9800",
    "danger": "#f44336",
    "info": "#2196f3",
    "border": "#f8bbd9"
  },
  "radii": {
    "sm": "12px",
    "md": "18px",
    "lg": "24px",
    "xl": "32px"
  },
  "spacing": {
    "xs": "6px",
    "sm": "12px",
    "md": "18px",
    "lg": "24px",
    "xl": "32px"
  },
  "typography": {
    "fontFamily": "'Noto Sans Arabic', 'Poppins', 'sans-serif'",
    "baseSize": 16,
    "lineHeight": 1.6,
    "weight": {
      "normal": 300,
      "medium": 500,
      "bold": 700
    }
  },
  "three": {
    "background": "#fff0f5",
    "camera": {
      "position": [0, 1.2, 3.8],
      "fov": 45
    },
    "lights": [
      { "type": "ambient", "intensity": 0.8 },
      { "type": "directional", "intensity": 1.2, "position": [2, 3, 2], "color": "#ffb3d9" },
      { "type": "point", "intensity": 0.8, "position": [-2, 2, 1], "color": "#e91e63" }
    ],
    "model": {
      "example": "heart",
      "scale": [1.3, 1.3, 1.3],
      "position": [0, -0.1, 0],
      "rotation": [0, 15, 0],
      "autoRotate": true,
      "rotationSpeed": 0.3
    },
    "effects": {
      "bloom": { "enabled": true, "intensity": 0.9 },
      "fog": { "color": "#fff0f5", "near": 5, "far": 18 },
      "shadow": { "enabled": true, "bias": -0.0003, "mapSize": [1024, 1024], "radius": 2 }
    }
  },
  "components": {
    "button": {
      "radius": "lg",
      "height": 46,
      "paddingX": "2rem",
      "variants": ["solid", "outline", "ghost"]
    },
    "card": {
      "radius": "xl",
      "shadow": "0 12px 32px rgba(233, 30, 99, 0.15)"
    },
    "input": { "radius": "lg" },
    "badge": { "radius": "lg" }
  }
}