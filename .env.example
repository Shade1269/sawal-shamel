# Supabase Configuration
# Copy this file to .env and fill in your values
# IMPORTANT: Never commit .env file to git!

VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_VAPID_PUBLIC_KEY=your_vapid_public_key_here

# Feature Flags
USE_ECOMMERCE_FLOW=true
# Inventory Configuration
DEFAULT_WAREHOUSE_CODE=MAIN

# Admin Configuration
VITE_ADMIN_EMAIL=admin@example.com

# Firebase Configuration (if using Firebase integration)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id_here

# Note: In Lovable, use Supabase Secrets for sensitive data instead of .env files
# This .env.example is provided for local development outside of Lovable
